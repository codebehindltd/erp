    </asp:ScriptManager>
    <div id="popUpDiv" class="hiddenDiv">
        <div id="popUpDivContainer">
            <div class="popupHeaderBox">
                <div class="popupTitle">
                </div>
                <div class="btnClose">
                    <a href="javascript:void(0)" onclick="javascript:popup(-1);">Close</a></div>
                <div class="clearCls">
                </div>
            </div>
            <div class="popupContentBox">
            </div>
        </div>
    </div>
    <div class="container-fluid" style="padding-left: 5px; padding-right: 5px;">
        <div class="row-fluid restaurantPageHeader" style="margin: 0px; padding: 0px;">
            <div class="span2" style="padding: 10px 20px;">
                <img src="../StyleSheet/images/Innboard-Logo_White.png" class=" InnBoardIcon" alt="logo" />
            </div>
            <div class="span6">
                <asp:ContentPlaceHolder ID="ContentPlaceHolder2" runat="server">
                </asp:ContentPlaceHolder>
            </div>
            <div class="span4">
                <div class="navbar navbar-inner" style="border: 0px;">
                    <ul class="nav pull-right">
                        <li style="color: #fff; vertical-align: middle; padding-top: 9px;">
                            <asp:Label ID="lblLoggedInUser" runat="server" Text=""></asp:Label>
                        </li>
                        <li style="color: #fff; vertical-align: middle;"><a href="javascript:void()"
                            class="hidden-phone visible-tablet visible-desktop">
                            <asp:Label ID="lblDayOpenDate" runat="server" Text=""></asp:Label></a> </li>
                        <li><a href="/Logout.aspx?LoginType=RestaurentLogin" class="hidden-phone visible-tablet visible-desktop">
                            <i class="icon-book"></i>Logout</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid" style="padding-left: 5px; padding-right: 5px; min-height: 80vh;">
        <div class="row-fluid" style="margin: 0px; padding: 0px;">
            <div id="spinner" class="spinner" style="display: none;">
                <img id="img-spinner" src="../Images/spinner.gif" alt="Loading ..." />
            </div>
            <div>
                <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                </asp:ContentPlaceHolder>
            </div>
