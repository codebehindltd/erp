                <p class="span6" style="text-align: right; margin: 0px; padding: 0px; width: 49.718%;">
                    &copy; 2013 <a href="http://www.innboard.com" target="_blank">innboard.com</a></p>
            </div>
        </div>
    </div>
    </form>
    <script type="text/javascript">
        var $loading = $('#spinner').hide();
        $(document)
          .ajaxStart(function () {
              $loading.show();
          })
          .ajaxStop(function () {
              $loading.hide();
          })
          .ajaxError(function () {
              $loading.hide();
          });

